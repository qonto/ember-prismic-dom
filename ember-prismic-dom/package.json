{
  "name": "ember-prismic-dom",
  "version": "4.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/qonto/ember-prismic-dom",
  "license": "MIT",
  "author": "",
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'pnpm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "prepack": "rollup --config",
    "prepare": "rollup --config"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.0.0",
    "@prismicio/richtext": "^1.1.0",
    "ember-element-helper": "^0.8.4"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-syntax-decorators": "^7.17.0",
    "@babel/runtime": "^7.23.2",
    "@embroider/addon-dev": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "concurrently": "^8.2.1",
    "ember-template-lint": "^5.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "rollup": "^3.22.0",
    "rollup-plugin-copy": "^3.5.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/prismic/children.js": "./dist/_app_/components/prismic/children.js",
      "./components/prismic/dom.js": "./dist/_app_/components/prismic/dom.js",
      "./components/prismic/element.js": "./dist/_app_/components/prismic/element.js",
      "./components/prismic/image.js": "./dist/_app_/components/prismic/image.js",
      "./helpers/eq.js": "./dist/_app_/helpers/eq.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  }
}
